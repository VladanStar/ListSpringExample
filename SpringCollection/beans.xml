<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="basket" class="com.vladancupric.FruitBasket">
		<constructor-arg type="String" value="myBasket" name="name"></constructor-arg>
		<constructor-arg>
			<list>
				<value>Orange</value>
				<value>Apple</value>
				<value>PineApple</value>
				<value>Orange</value>
				<value>WatherMelone</value>
				<value>Melone</value>
				<value>Kiwi</value>
			</list>
		</constructor-arg>
	</bean>
</beans>
